<table summary="user form fields">
  <tr>
    <th>Name</th>
    <td><%= f.text_field(:name) %></td>
  </tr>
  <tr>
    <th>Username</th>
    <td><%= f.text_field(:username) %></td>
  </tr>
  <tr>
    <th>Password</th>
    <td><%= f.text_field(:password_digest) %></td>
  </tr>
  <tr>
    <th>email</th>
    <td><%= f.text_field(:email) %></td>
  </tr>
</table>